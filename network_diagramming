<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <title>Network Diagram – PNG export with icons</title>
  <script src="https://d3js.org/d3.v7.min.js"></script>
  <style>
    body {
      font-family: sans-serif;
      margin: auto;
    }

    .node text {
      font: 14px sans-serif;
      fill: darkblue
    }

    .link {
      fill: none;
      stroke: #ccc;
      stroke-width: 2
    }

    #download {
      margin-top: 10px;
      padding: 6px 12px
    }

    button {
      color: navy blue;
    }

    .diagram {
      background-color: #fff;
      /* keeps the SVG on a white panel even if the page isn’t */
      border: 3pt solid gray;
      /* ← add a style keyword */
      display: inline-block;
      /* lets the border wrap tightly around the SVG */
      padding: 8px;
      /* (}optional) little breathing room inside the frame */
    }
  </style>
</head>

<body>
  <center>

    <h2>Upload CSV → View & Download Diagram</h2>
    <input type="file" id="upload" accept=".csv">
    <label for="orient">Orientation:</label>
    <select id="orient">
      <option value="horizontal">Horizontal</option>
      <option value="vertical">Vertical</option>
    </select>
    <button id="build-diagram"> Build the Diagram</button>

    <button id="download">Download PNG</button>
    <br><br>
    <div class="diagram">
      <svg id="network"></svg>
    </div>
  </center>

  <script>
    /* --- helper so the PNG download has a white background --- */
    function ensureWhiteBackground(svg, w, h) {
      svg.insert('rect', ':first-child')
        .attr('width', w)
        .attr('height', h)
        .attr('fill', '#fff');
    }

    /* -------------------------------------------------------
       0) One‑time list of your icon files.
          Add/replace entries as needed.
       ------------------------------------------------------- */
    const ICON_SRC = {
      wifi_ap: 'data:image/png;base64,',
      router: 'data:image/png;base64,',
      camera: 'data:image/png;base64,',
      server: 'data:image/jpeg;base64,',
      printer: 'data:image/png;base64,',
      switch: 'data:image/png;base64,',
      desktop: 'data:image/png;base64,',
      laptop: 'data:image/png;base64,',
      phone: 'data:image/png;base64,',
      tablet: 'data:image/png;base64,',
      tv: 'data:image/png;base64,',
      default: 'data:image/png;base64,'
    };

    /* -------------------------------------------------------
   Formating and building a legend for the diagram. 
   ------------------------------------------------------- */
    function buildLegend() { 
      const legend = svg.append('g')
        .attr('class', 'legend')
        .attr('transform', `translate(20, 20)`);

      // Dashed Line = Wireless
      legend.append('line')
        .attr('x1', 0).attr('y1', 0)
        .attr('x2', 40).attr('y2', 0)
        .attr('stroke', '#3b82f6')
        .attr('stroke-width', 4)
        .attr('stroke-dasharray', '6,4');

      legend.append('text')
        .attr('x', 50).attr('y', 5)
        .text('Wireless Connection')
        .attr('alignment-baseline', 'middle')
        .style('fill', '#000')
        .style('font-size', '14px');

      // Solid Line = Wired
      legend.append('line')
        .attr('x1', 0).attr('y1', 25)
        .attr('x2', 40).attr('y2', 25)
        .attr('stroke', '#888')
        .attr('stroke-width', 4);

      legend.append('text')
        .attr('x', 50).attr('y', 30)
        .text('Wired Connection')
        .attr('alignment-baseline', 'middle')
        .style('fill', '#000')
        .style('font-size', '14px');

      // Circle border = Infected
      legend.append('circle')
        .attr('cx', 20).attr('cy', 55).attr('r', 10)
        .attr('fill', '#fff')
        .attr('stroke', 'red')
        .attr('stroke-width', 2);

      legend.append('text')
        .attr('x', 50).attr('y', 60)
        .text('Infected Device')
        .attr('alignment-baseline', 'middle')
        .style('fill', '#000')
        .style('font-size', '14px');
    }

    /* ---------- icon helpers unchanged ---------- */
    function pickIconPath(name) {
      if (name.includes('AccessPoint')) return ICON_SRC.wifi_ap;
      if (name.includes('Router')) return ICON_SRC.router;
      if (name.includes('Camera')) return ICON_SRC.camera;
      if (name.includes('Server')) return ICON_SRC.server;
      if (name.includes('Switch')) return ICON_SRC.switch;
      if (name.includes('Printer')) return ICON_SRC.printer;
      if (name.includes('Desktop') ||
        name.includes('Workstation')) return ICON_SRC.desktop;
      if (name.includes('Laptop')) return ICON_SRC.laptop;
      if (name.includes('Phone')) return ICON_SRC.phone;
      if (name.includes('Tablet')) return ICON_SRC.tablet;
      if (name.includes('TV')) return ICON_SRC.tv;
      return ICON_SRC.default;
    }
    function fileToDataURL(path) {
      return new Promise((res, rej) => {
        const img = new Image();
        img.onload = () => {
          const c = document.createElement('canvas');
          c.width = img.width; c.height = img.height;
          c.getContext('2d').drawImage(img, 0, 0);
          res(c.toDataURL('image/png'));
        };
        img.onerror = rej;
        img.src = path;
      });
    }
    const ICON_CACHE = {};
    async function getIconDataURI(path) {
      if (!path) return null;
      if (ICON_CACHE[path]) return ICON_CACHE[path];
      const uri = await fileToDataURL(path);
      ICON_CACHE[path] = uri;
      return uri;
    }

    /* ---------- D3 diagram ---------- */
    // --- grab the SVG as a D3 selection ---
    const svg = d3.select('#network');
    const select = document.getElementById('orient');

    // reusable helper
    function applySize(mode) {
      const w = mode === 'horizontal' ? 1200 : 800;
      const h = mode === 'horizontal' ? 800 : 1000;

      // D3 → chain .attr()
      svg.attr('width', w)
        .attr('height', h);

      // if other code needs these numbers, expose or recompute them here
      window.W = w;   // or let W = w; H = h; if they're local
      window.H = h;
    }

    // initial call (matches the <option> selected in the markup)
    applySize(select.value);

    // run again whenever the user flips the dropdown
    select.addEventListener('change', e => applySize(e.target.value));

    /* =====================================
    - When the CSV File has been uploaded -
    ===================================== */
    let parsedData = null;  // holds the CSV after selection

    document.getElementById('upload').addEventListener('change', e => {
      const f = e.target.files[0];
      if (!f) {
        parsedData = null;
        document.getElementById('build-diagram').disabled = true;
        return;
      }

      const r = new FileReader();
      r.onload = ev => {
        parsedData = d3.csvParse(ev.target.result);   // just store it
        document.getElementById('build-diagram').disabled = false;  // ready!
      };
      r.readAsText(f);
    });


    document.getElementById('build-diagram').addEventListener('click', () => {
      if (!parsedData) return;      // safety check
      draw(parsedData);             // now build the diagram
    });

    /* =====================================
     - Building the Network Diagram  -
     ===================================== */
    async function draw(data) {
      svg.selectAll('*').remove();          // clear previous diagram
      ensureWhiteBackground(svg, W, H);     // now defined ✔

      data = data.filter(d => d.Device && d.Connectedto && d.Device !== '0');

      const links = data.map(d => {
        const source = d.Connectedto.trim();
        const target = d.Device.trim();
        const type = d.conntection_type?.trim().toLowerCase();  // <- e.g., 'wired', 'wireless'
        return {
          source,
          target,
          conntection_type: type
        };
      });
      // Build infection map
      const infectionMap = {};
      data.forEach(d => {
        if (d.Device && d.Infected !== undefined) {
          infectionMap[d.Device.trim()] = d.Infected.toLowerCase() === 'true';
        }
      });

      // Create nodes with infection info
      const nodes = Array.from(new Set(links.flatMap(l => [l.source, l.target])))
        .map(id => ({
          id,
          iconPath: pickIconPath(id),
          Infected: infectionMap[id] || false
        }));

      const NODE_R = 50;
      const PADDING = 6;
      const DISTANCE = 180;

      const sim = d3.forceSimulation(nodes)
        .force('link', d3.forceLink(links).id(d => d.id).distance(DISTANCE))
        .force('charge', d3.forceManyBody().strength(-500))
        .force('collision', d3.forceCollide().radius(NODE_R + PADDING).iterations(2))
        .force('center', d3.forceCenter(W / 2, H / 2));

      // lines
      const link = svg.append('g')
        .attr('class', 'link')
        .selectAll('line')
        .data(links).enter().append('line')
        .attr('stroke', d => d.conntection_type === 'wi-fi' ? '#3b82f6' : "#888")
        .attr('stroke-width', 4)
        .attr('stroke-dasharray', d => d.conntection_type === 'wi-fi' ? '6,4' : null);

      const node = svg.append('g')
        .selectAll('g').data(nodes).enter().append('g')
        .attr('class', 'node').call(drag(sim));

      node.append('circle')
        .attr('r', NODE_R)
        .attr('fill', '#fff')
        .attr('stroke', d => d.Infected ? 'red' : 'transparent') // ✅ lowercase "Infected"
        .attr('stroke-width', 1);

      node.append('image')
        .attr('href', d => d.iconPath)
        .attr('width', 50).attr('height', 50).attr('x', -25).attr('y', -25);

      node.append('text')
        .attr('y', 35).attr('text-anchor', 'middle')
        .text(d => d.id);

      sim.on('tick', () => {
        node.attr('transform', d => {
          d.x = Math.max(NODE_R, Math.min(W - NODE_R, d.x));
          d.y = Math.max(NODE_R, Math.min(H - NODE_R, d.y));
          return `translate(${d.x},${d.y})`;
        });
        link.attr('x1', d => d.source.x).attr('y1', d => d.source.y)
          .attr('x2', d => d.target.x).attr('y2', d => d.target.y);
      });

      sim.on('end', async () => {
        const imgs = svg.selectAll('image').nodes();
        await Promise.all(imgs.map(async img => {
          const src = img.getAttribute('href');
          try {
            const dataURI = await getIconDataURI(src);
            if (dataURI) {
              img.setAttribute('href', dataURI);
              img.setAttributeNS('http://www.w3.org/1999/xlink', 'href', dataURI);
            }
          } catch (_) { console.warn('icon inline failed', src); }
        }));
        document.getElementById('download').disabled = false;
      });
      buildLegend();
    }



    /* drag unchanged */
    function drag(sim) {
      return d3.drag()
        .on('start', (e, d) => { if (!e.active) sim.alphaTarget(0.3).restart(); d.fx = d.x; d.fy = d.y; })
        .on('drag', (e, d) => { d.fx = e.x; d.fy = e.y; })
        .on('end', (e, d) => { if (!e.active) sim.alphaTarget(0); d.fx = d.fy = null; });
    }


    /* ===========================================
     - DOwnloading the created  Network Diagram  -
     ========================================== */

    /* download unchanged */
    document.getElementById('download').addEventListener('click', () => {
      const svgEl = document.getElementById('network');
      const svgStr = new XMLSerializer().serializeToString(svgEl);
      const canvas = document.createElement('canvas');
      canvas.width = svgEl.clientWidth || svgEl.width.baseVal.value;
      canvas.height = svgEl.clientHeight || svgEl.height.baseVal.value;
      const ctx = canvas.getContext('2d');
      const img = new Image();
      img.onload = () => {
        ctx.drawImage(img, 0, 0);
        const a = document.createElement('a');
        a.download = 'network_diagram.png';
        a.href = canvas.toDataURL('image/png');
        a.click();
      };
      img.src = 'data:image/svg+xml;base64,' + btoa(unescape(encodeURIComponent(svgStr)));
    });
  </script>
</body>

</html>
